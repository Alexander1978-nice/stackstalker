{
  "manifest_version": 2
  , "name": "StackStalker"
  , "version": "3.0"
  , "description": "Monitor any question from any Stack Exchange site and see when it is updated!"
  , "icons": {
    "128": "images/logo.png"
  }
  , "background": {
    "scripts": [
      "jquery.min.js"
      , "apis.js"
      , "question.js"
      , "background.js"
    ]
  }
  , "browser_action": {
    "default_icon": "images/icon.png"
    , "default_popup": "popup.html"
  }
  , "permissions": [
  	"tabs"
    , "unlimitedStorage"
    , "storage"
    , "http://*.stackoverflow.com/"
    , "http://*.superuser.com/"
    , "http://*.stackapps.com/"
    , "http://*.serverfault.com/"
    , "http://*.stackexchange.com/"
  ]
  , "author": "Jason Edelman"
  , "homepage_url": "http://jasonedelman.com"
}